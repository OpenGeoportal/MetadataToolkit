<?xml version="1.0" encoding="ISO-8859-1"?>
<geonet>
	<services package="org.fao.geonet">

		<service name="portal.sru">
			<documentation>Portal SRU search.</documentation>
			<class name="org.fao.geonet.services.main.SRUSearch">
				<param name="maxSummaryKeys" value="10"/>
				<param name="maxHitsInSummary" value="1000"/>
			</class>
			<output sheet="../xslt/services/sru/portal-srusearch.xsl"
				contentType="text/xml; charset=UTF-8"/>
		</service>
	</services>
</geonet>
